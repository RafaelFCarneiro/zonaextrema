  "scripts": {
    "start": "watchify -o public/assets/js/bundle.js -v -d app/app.js",
    "build": "browserify app/app.js -t [envify --NODE_ENV production] | uglifyjs -cm | babel --optional optimisation.react.inlineElements --optional optimisation.react.constantElements > public/assets/js/bundle.min.js"
