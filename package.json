    "sequelize-cli": "^2.3.1",
    "stylus": "^0.54.2",
    "watchify": "^3.3.1"
  },
  "scripts": {
    "watch:css": "stylus -w -c styles/main.styl -o public/assets/css/main.css",
    "watch:js": "watchify -o public/assets/js/bundle.js -v -d app/app.js",
    "build:js": "browserify app/app.js -t [envify --NODE_ENV production] | uglifyjs -cm | babel --optional optimisation.react.inlineElements --optional optimisation.react.constantElements > public/assets/js/bundle.min.js",
    "watch:all": "parallelshell 'npm run watch:js' 'npm run watch:css'",
    "server": "PORT=4000 node server.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "blacklist": [
            "regenerator"
          ]
        }
      ],
      "envify"
    ]
  }
}
